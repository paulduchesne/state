PREFIX ont: <https://paulduchesne.github.io/personal-premis/ontology/>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX premis: <http://www.loc.gov/premis/rdf/v3/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

# owl:Class

<https://paulduchesne.github.io/personal-premis/ontology/ReadingEvent>
    a owl:Class ;
    rdfs:label "Reading Event"@en ;
    rdfs:comment "Event type concerning the act of reading."@en ;
    rdfs:subClassOf premis:Event ;
.

<https://paulduchesne.github.io/personal-premis/ontology/Book>
    a owl:Class ;
    rdfs:label "Book"@en ; 
    rdfs:comment "Printed material."@en ;  
.

# owl:ObjectProperty

<https://paulduchesne.github.io/personal-premis/ontology/hasAuthor>
    a owl:ObjectProperty ;
    rdfs:label "Has Author"@en ; 
    rdfs:comment "Person responsible for the text of a book."@en ;
    rdfs:domain ont:Book ;   
    rdfs:range premis:Person ;   
.

<https://paulduchesne.github.io/personal-premis/ontology/hasParticipant>
    a owl:ObjectProperty ;
    rdfs:label "Has Participant"@en ;  
    rdfs:comment "Indicate entities who participate, most often in an event."@en ;
.

<https://paulduchesne.github.io/personal-premis/ontology/worksAt>
    a owl:ObjectProperty ;
    rdfs:label "Works At"@en ; 
    rdfs:comment "Indicate the organisation where someone works."@en ;
    rdfs:domain premis:Person ;   
    rdfs:range premis:Organization ;   
.

<https://paulduchesne.github.io/personal-premis/ontology/attended>
    a owl:ObjectProperty ;
    rdfs:label "Attended"@en ; 
    rdfs:comment "To attend an event."@en ; 
.

# owl:DatatypeProperty

<https://paulduchesne.github.io/personal-premis/ontology/startDate>
    a owl:DatatypeProperty ;
    rdfs:label "Start Date"@en ;  
    rdfs:comment "Start of a period of time."@en ;
.

<https://paulduchesne.github.io/personal-premis/ontology/endDate>
    a owl:DatatypeProperty ;
    rdfs:label "End Date"@en ;  
    rdfs:comment "End of a period of time."@en ;
.

<https://paulduchesne.github.io/personal-premis/ontology/wikidataId>
    a owl:DatatypeProperty ;
    rdfs:label "Wikidata ID"@en ;  
    rdfs:comment "Identifier use by Wikidata."@en ;
.

