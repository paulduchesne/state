PREFIX ont: <https://paulduchesne.github.io/state/ontology/>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

# owl:Class

<https://paulduchesne.github.io/state/ontology/Person>
    a owl:Class ;
    rdfs:label "Person"@en ; 
    rdfs:comment "Generally speaking, some Homo sapian."@en ;  
.

<https://paulduchesne.github.io/state/ontology/Organisation>
    a owl:Class ;
    rdfs:label "Organisation"@en ; 
    rdfs:comment "Generally speaking, some ordered collection of Homo sapians."@en ;  
.

<https://paulduchesne.github.io/state/ontology/Institution>
    a owl:Class ;
    rdfs:label "Institution"@en ; 
    rdfs:comment "An organisation of people, officially mandated by government."@en ;  
    rdfs:subClassOf ont:Organisation ;
.

<https://paulduchesne.github.io/state/ontology/School>
    a owl:Class ;
    rdfs:label "School"@en ; 
    rdfs:comment "An organisation of people, intended for the education of people."@en ;  
    rdfs:subClassOf ont:Organisation ;
.

<https://paulduchesne.github.io/state/ontology/Band>
    a owl:Class ;
    rdfs:label "Band"@en ; 
    rdfs:comment "An organisation of people, specifically to play music together."@en ;  
    rdfs:subClassOf ont:Organisation ;
.

<https://paulduchesne.github.io/state/ontology/Location>
    a owl:Class ;
    rdfs:label "Location"@en ; 
    rdfs:comment "An entity which is most significantly defined geographically."@en ;  
.

<https://paulduchesne.github.io/state/ontology/Event>
    a owl:Class ;
    rdfs:label "Event"@en ; 
    rdfs:comment "An entity which is most significantly defined by a moment, or duration, in time."@en ;  
.

<https://paulduchesne.github.io/state/ontology/Marriage>
    a owl:Class ;
    rdfs:label "Marriage"@en ; 
    rdfs:comment "Social event in which two people are declared to be married."@en ;  
    rdfs:subClassOf ont:Event ;
.

<https://paulduchesne.github.io/state/ontology/Birth>
    a owl:Class ;
    rdfs:label "Birth"@en ; 
    rdfs:comment "Biological event, the start of a person's life."@en ;  
    rdfs:subClassOf ont:Event ;
.

<https://paulduchesne.github.io/state/ontology/Death>
    a owl:Class ;
    rdfs:label "Death"@en ; 
    rdfs:comment "Biological event, the end of a person's life."@en ;  
    rdfs:subClassOf ont:Event ;
.

<https://paulduchesne.github.io/state/ontology/ReadingEvent>
    a owl:Class ;
    rdfs:label "Reading Event"@en ; 
    rdfs:comment "The process of reading a book."@en ;  
    rdfs:subClassOf ont:Event ;
.

<https://paulduchesne.github.io/state/ontology/OrganisationalEvent>
    a owl:Class ;
    rdfs:label "Organisational Event"@en ; 
    rdfs:comment "Annual event tied to an organisation. Also known as a Conference, a Congress or a Symposium, the distinguishing facets escape me."@en ;  
    rdfs:subClassOf ont:Event ;
.

<https://paulduchesne.github.io/state/ontology/Object>
    a owl:Class ;
    rdfs:label "Object"@en ; 
    rdfs:comment "An inanimate entity."@en ;  
.

<https://paulduchesne.github.io/state/ontology/Book>
    a owl:Class ;
    rdfs:label "Book"@en ; 
    rdfs:comment "Printed material."@en ;  
    rdfs:subClassOf ont:Object ;
.

<https://paulduchesne.github.io/state/ontology/Album>
    a owl:Class ;
    rdfs:label "Album"@en ; 
    rdfs:comment "Manifestation of music."@en ;  
    rdfs:subClassOf ont:Object ;
.

# owl:ObjectProperty

<https://paulduchesne.github.io/state/ontology/hasParticipant>
    a owl:ObjectProperty ;
    rdfs:label "Has Participant"@en ;  
    rdfs:comment "Indicate the entities who participated in an event."@en ;
    rdfs:domain ont:Event ;   
    rdfs:range ont:Person, ont:Object ;   
    owl:inverseOf ont:participatedIn ; 
.

<https://paulduchesne.github.io/state/ontology/participatedIn>
    a owl:ObjectProperty ;
    rdfs:label "Participated In"@en ;  
    rdfs:comment "Indicate the event participated in by an entity."@en ;
    rdfs:domain ont:Person, ont:Object ;   
    rdfs:range ont:Event ;
    owl:inverseOf ont:hasParticipant ; 
.

<https://paulduchesne.github.io/state/ontology/memberOf>
    a owl:ObjectProperty ;
    rdfs:label "Member Of"@en ; 
    rdfs:comment "Indicate the organisation of which a person is a member."@en ;
    rdfs:domain ont:Person ;   
    rdfs:range ont:Organisation ;   
    owl:inverseOf ont:hasMember ;   
.

<https://paulduchesne.github.io/state/ontology/hasMember>
    a owl:ObjectProperty ;
    rdfs:label "Has Member"@en ; 
    rdfs:comment "Indicate the person with membership in an organisation."@en ;
    rdfs:domain ont:Organisation ;   
    rdfs:range ont:Person ;  
    owl:inverseOf ont:memberOf ;
.

<https://paulduchesne.github.io/state/ontology/hasAuthor>
    a owl:ObjectProperty ;
    rdfs:label "Has Author"@en ; 
    rdfs:comment "Person responsible for the text of a book."@en ;
    rdfs:domain ont:Book ;   
    rdfs:range ont:Person ;  
    owl:inverseOf ont:authorOf ; 
.

<https://paulduchesne.github.io/state/ontology/authorOf>
    a owl:ObjectProperty ;
    rdfs:label "Author Of"@en ; 
    rdfs:comment "Books written by the author."@en ;
    rdfs:domain ont:Person ;   
    rdfs:range ont:Book ;   
    owl:inverseOf ont:hasAuthor ;
.

<https://paulduchesne.github.io/state/ontology/hasCreator>
    a owl:ObjectProperty ;
    rdfs:label "Has Creator"@en ; 
    rdfs:comment "Agent responsible for the creation of an artwork."@en ;
    rdfs:domain ont:Album ;   
    rdfs:range ont:Person, ont:Organisation ;  
    owl:inverseOf ont:authorOf ; 
.

<https://paulduchesne.github.io/state/ontology/creatorOf>
    a owl:ObjectProperty ;
    rdfs:label "Creator Of"@en ; 
    rdfs:comment "Artwork created by the agent."@en ;
    rdfs:domain ont:Person, ont:Organisation ;   
    rdfs:range ont:Album ;   
    owl:inverseOf ont:hasAuthor ;
.

<https://paulduchesne.github.io/state/ontology/hasLocation>
    a owl:ObjectProperty ;
    rdfs:label "Has Location"@en ; 
    rdfs:comment "Location where an event was held."@en ;
    rdfs:domain ont:Event ;   
    rdfs:range ont:Location ;  
    owl:inverseOf ont:locationOf ; 
.

<https://paulduchesne.github.io/state/ontology/locationOf>
    a owl:ObjectProperty ;
    rdfs:label "Location Of"@en ; 
    rdfs:comment "Event held in a location."@en ;
    rdfs:domain ont:Location ;   
    rdfs:range ont:Event ;   
    owl:inverseOf ont:hasLocation ;
.

# owl:DatatypeProperty

<https://paulduchesne.github.io/state/ontology/startDate>
    a owl:DatatypeProperty ;
    rdfs:label "Start Date"@en ;  
    rdfs:comment "Start of a period of time."@en ;
.

<https://paulduchesne.github.io/state/ontology/endDate>
    a owl:DatatypeProperty ;
    rdfs:label "End Date"@en ;  
    rdfs:comment "End of a period of time."@en ;
.

<https://paulduchesne.github.io/state/ontology/wikidataIdentifier>
    a owl:DatatypeProperty ;
    rdfs:label "Wikidata Identifier"@en ;  
    rdfs:comment "Identifier used by Wikidata."@en ;
.

<https://paulduchesne.github.io/state/ontology/identifier>
    a owl:DatatypeProperty ;
    rdfs:label "identifier"@en ;  
    rdfs:comment ""@en ;
.


